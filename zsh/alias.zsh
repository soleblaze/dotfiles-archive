#!/bin/zsh

# Various aliases
alias free="free -m"
alias grep="grep --color -i"
alias grepc="grep --color=always -i"
alias sz="source ~/.zshrc"

# VIM aliases
alias vi=nvim
alias svi="sudoedit"

# extra dots
alias -g ...="../.."
